@model TourGuide.Models.Place

@{
    ViewData["Title"] = "PlaceDetails";
    var all_photos = Model.Photos;
}
<div class="jumbotron">
    <h1>@Model.PlaceName ( @Model.Country.CountryName ) </h1>
    <p>@Model.Description</p>
</div>
<h1>Gallery</h1>
<div class="card-columns">
    @foreach (var photo in all_photos)
    {
        <div class="card" style="height:200px">
            <img src="~/places/@photo.PhotoID@photo.ExtName" style="width:100%;height:100%" />
        </div>
    }

</div>